<configurator>
  <domain name="CHIP"/>

  <enum name="PositioningEnum" type="enum8">
    <cluster code="0x0CD0"/> <!-- Closure Operational State -->
    <item value="0x00" name="FullyClosed"/>
    <item value="0x01" name="FullyOpened"/>
    <item value="0x02" name="PartiallyOpened"/>
    <item value="0x03" name="OneQuarterOpened"/>
    <item value="0x04" name="HalfOpened"/>
    <item value="0x05" name="ThreeQuarterOpened"/>
    <item value="0x06" name="OpenedForPedestrian"/>
    <item value="0x07" name="OpenedForVentilation"/>
    <item value="0x08" name="OpenedAtSignature"/>
  </enum>

  <struct name="OverallStateStruct">
    <cluster code="0x0CD0"/> <!-- Closure Operational State -->
    <item name="Positioning" type="PositioningEnum"    optional="true"/>
    <item name="Latching"    type="LatchingEnum"       optional="true"/>
    <item name="Speed"       type="ThreeLevelAutoEnum" optional="true"/>
  </struct>

  <enum name="ProtectiveStateEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x00" name="Dropped"/>
    <item value="0x01" name="Raised"/>
  </enum>

  <enum name="RestingProcedureEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x00" name="DoNothing"/>
    <item value="0x01" name="ReturnToFullyOpened"/>
    <item value="0x02" name="ReturnToFullyClosed"/>
  </enum>

  <enum name="TriggerPositionEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x00" name="AtFullyClosed"/>
    <item value="0x01" name="AtFullyOpened"/>
    <item value="0x02" name="AtPartiallyOpened"/>
    <item value="0x03" name="AtVentilation"/>
    <item value="0x04" name="AtPedestrian"/>
  </enum>

  <enum name="TriggerConditionEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x00" name="AfterDelay"/>
    <item value="0x01" name="AfterApplicativeTrigger"/>
    <item value="0x02" name="AfterDelayOrApplicativeTrigger"/>
  </enum>

  <enum name="TagEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x00" name="CloseInFull"/>
    <item value="0x01" name="OpenInFull"/>
    <item value="0x02" name="OpenOneQuarter"/>
    <item value="0x03" name="OpenInHalf"/>
    <item value="0x04" name="OpenThreeQuarter"/>
    <item value="0x05" name="Pedestrian"/>
    <item value="0x06" name="Ventilation"/>
    <item value="0x07" name="Signature"/>
    <item value="0x08" name="SequenceNextStep"/>
    <item value="0x09" name="PedestrianNextStep"/>
  </enum>

  <enum name="OperationalStateEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x40" name="Calibrating"/>
    <item value="0x41" name="Protected"/>
    <item value="0x42" name="Disengaded"/>
    <item value="0x43" name="SetupRequired"/>
    <item value="0x44" name="PendingFallback"/>
  </enum>

  <enum name="ErrorStateEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x40" name="Blocked"/>
    <item value="0x41" name="ThermalProtected"/>
    <item value="0x42" name="MaintenanceRequired"/>
    <item value="0x43" name="IntegratedElement"/>
  </enum>

</configurator>
