<configurator>
  <domain name="CHIP"/>

  <enum name="OverallPositioningEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x00" name="NotApplicable"/>
    <item value="0x01" name="FullyOpened"/>
    <item value="0x02" name="PartiallyOpened"/>
    <item value="0x03" name="QuarterOpened"/>
    <item value="0x04" name="HalflyOpened"/>
    <item value="0x05" name="QuarterClosed"/>
    <item value="0x06" name="FullyClosed"/>
  </enum>

  <enum name="OverallLatchingEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x00" name="NotApplicable"/>
    <item value="0x01" name="NotLatched"/>
    <item value="0x02" name="PartiallyLatched"/>
    <item value="0x03" name="FullyLatched"/>
  </enum>

  <bitmap name="OverallStateBitmap" type="bitmap8">
    <cluster code="0x0CD0"/>
    <field mask="0x0F" name="Position"/>
    <field mask="0xF0" name="Latching"/>
  </bitmap>

  <enum name="ProtectiveStateEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x00" name="Dropped"/>
    <item value="0x01" name="Raised"/>
  </enum>

  <enum name="AutoBehaviorEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x00" name="Wait"/>
    <item value="0x01" name="ReturnToFullyOpened"/>
    <item value="0x02" name="ReturnToFullyClosed"/>
  </enum>

  <enum name="TagEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x00" name="OpenInFull"/>
    <item value="0x01" name="CloseOneQuarter"/>
    <item value="0x02" name="OpenInHalf"/>
    <item value="0x03" name="OpenOneQuarter"/>
    <item value="0x04" name="CloseInFull"/>
    <item value="0x05" name="Pedestrian"/>
    <item value="0x06" name="Ventilation"/>
    <item value="0x07" name="Signature"/>
    <item value="0x08" name="SequenceNextStep"/>
  </enum>

  <enum name="OperationalStateEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x40" name="Calibrating"/>
    <item value="0x41" name="Protected"/>
    <item value="0x42" name="Disengaded"/>
    <item value="0x43" name="SetupRequired"/>
  </enum>

  <enum name="ErrorStateEnum" type="enum8">
    <cluster code="0x0CD0"/>
    <item value="0x40" name="Blocked"/>
    <item value="0x41" name="ThermalProtected"/>
    <item value="0x42" name="InvalidSetup"/>
    <item value="0x43" name="IntegratedElement"/>
  </enum>

  <enum name="ThreeLevelAutoEnum" type="enum8">
    <cluster code="0x0CD0"/> <!-- Closure Operational State -->
    <cluster code="0x0CD1"/> <!-- Closure 1st Dimension -->
    <cluster code="0x0CD2"/> <!-- Closure 2nd Dimension -->
    <cluster code="0x0CD3"/> <!-- Closure 3rd Dimension -->
    <cluster code="0x0CD4"/> <!-- Closure 4th Dimension -->
    <cluster code="0x0CD5"/> <!-- Closure 5th Dimension -->
    <cluster code="0x0CD6"/> <!-- Closure 6th Dimension -->
    <cluster code="0x0CD7"/> <!-- Closure 7th Dimension -->
    <cluster code="0x0CD8"/> <!-- Closure 8th Dimension -->
    <item value="0x0" name="Auto"/>
    <item value="0x1" name="Low"/>
    <item value="0x2" name="Medium"/>
    <item value="0x3" name="High"/>
  </enum>
</configurator>
